<div id="sidebar-content">
    <div id="sidebar-small" class="" style="overflow: hidden; outline: none;" tabindex="5000">
      <!-- sidebar menu start-->
      <ul class="sidebar-menu" id="nav-accordion">
        <li>
          <%= link_to root_url(subdomain:@web.subdomain, :back_link => "true"), id: "sidebar-view" do %>
              <i class="fa fa-eye"></i>
          <% end %>
        </li>

        <li>
          <a id="sidebar-blocks" class="active" href="#">
            <i class="fa fa-th-large"></i>
          </a>
        </li>

        <li>
          <a id="sidebar-pages" href="#">
            <i class="fa fa-copy"></i>
          </a>
        </li>

        <li>
          <a id="sidebar-settings" href="#">
            <i class="fa fa-cog"></i>
          </a>
        </li>

        <li>
          <a id="sidebar-services" href="#">
            <i class="fa fa-sitemap"></i>
          </a>
        </li>

        <li>
          <a id="sidebar-publish" href="#">
            <i class="fa fa-upload"></i>
          </a>
        </li>

      </ul>
      <!-- sidebar menu end-->
    </div>
    <div id="sidebar-big" class="" style="overflow: hidden; outline: none;" tabindex="5000">

      <!-- BLOCKS CONTENT start -->
      <div class="sidebar-big-content" id="view-content">
        <h2>View</h2>
      </div>
      <!-- BLOCKS CONTENT end -->

      <!-- BLOCKS CONTENT start -->
        <div class="sidebar-big-content" id="blocks-content">
            <%= render 'webs/edit_panel/blocks' %>
        </div>
        <!-- BLOCKS CONTENT end -->

      <!-- PAGES CONTENT start -->
      <div class="sidebar-big-content" id="pages-content">
        <%= render 'webs/edit_panel/pages' %>
      </div>
      <!-- PAGES CONTENT end -->

      <!-- SETTINGS CONTENT start -->
      <div class="sidebar-big-content" id="settings-content">
        <%= render 'webs/edit_panel/settings' %>
      </div>
      <!-- SETTINGS CONTENT end -->

      <!-- SERVICES CONTENT start -->
      <div class="sidebar-big-content" id="services-content">
        <%= render 'webs/edit_panel/ext_services' %>
      </div>
      <!-- SERVICES CONTENT end -->

      <!-- PUBLISH CONTENT start -->
      <div class="sidebar-big-content" id="publish-content">
        <%= render 'webs/edit_panel/publish' %>
      </div>
      <!-- PUBLISH CONTENT end -->

    </div>
</div>